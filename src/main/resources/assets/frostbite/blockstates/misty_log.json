{
  "multipart": [
    {
      "apply": {
        "model": "frostbite:block/misty_log_horizontal",
        "x": 90,
        "y": 90
      },
      "when": {
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "frostbite:block/misty_log"
      },
      "when": {
        "axis": "y"
      }
    },
    {
      "apply": {
        "model": "frostbite:block/misty_log_horizontal",
        "x": 90
      },
      "when": {
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "frostbite:block/misty_log_horizontal_emissive",
        "x": 90,
        "y": 90
      },
      "when": {
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "frostbite:block/misty_log_emissive"
      },
      "when": {
        "axis": "y"
      }
    },
    {
      "apply": {
        "model": "frostbite:block/misty_log_horizontal_emissive",
        "x": 90
      },
      "when": {
        "axis": "z"
      }
    }
  ]
}